{
  "name": "daxc-i",
  "version": "0.2.2-alpha.0",
  "description": "Identity Access implementing daxc Access Control",
  "main": "./dst/index.js",
  "typings": "./typings/index.d.ts",
  "scripts": {
    "build": "npm run cleanup && npm run build-ts",
    "build-pack": "npm run build && npm run webpack",
    "test": "npm run cleanup && npm run test-ts",
    "test-cvr": "npm run cleanup && npm run test-cvr-ts",
    "test-ts": "npm run build-ts && npm run test-all",
    "test-cvr-ts": "npm run build-ts && npm run test-all-cvr",
    "build-ts": "npm run tslint && tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "cleanup": "node scripts/cleanup",
    "aftertests": "node scripts/aftertests",
    "test-all": "npm run test-mocha",
    "test-all-cvr": "npm run test-mocha-cvr",
    "test-mocha": "mocha dst --recursive",
    "test-mocha-cvr": "nyc mocha dst --recursive",
    "coverage": "npm run coveralls",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "cix-lic": "cix-lic --conf",
    "webpack": "webpack --mode=production"
  },
  "author": "Architecode Corporation",
  "license": "MIT",
  "repository": "git+https://github.com/architecode/daxc",
  "dependencies": {
    "daxc-common": "0.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.18",
    "@types/mocha": "8.2.2",
    "@types/node": "15.6.0",
    "@types/sinon": "10.0.0",
    "chai": "4.3.4",
    "coveralls": "3.1.0",
    "mocha": "8.4.0",
    "mock-require": "3.0.3",
    "nyc": "15.1.0",
    "shelljs": "0.8.4",
    "sinon": "11.0.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4",
    "webpack": "5.37.1",
    "webpack-cli": "4.7.0"
  }
}
